{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "baseUrl": ".",
    "paths": {
      "@/*": ["services/app/src/*"]
    }
  },
  "include": [
    "libs/types.app/**/*.ts",
    "libs/types.utility/**/*.ts",

    "services/app/env.d.ts",
    "services/app/src/**/*.ts",
    "services/app/src/**/*.vue"
  ]
}
